{
  "name": "sample-app",
  "version": "1.0.0",
  "description": "sample description of the app",
  "main": "index.js",
  "scripts": {
    "build:dev": "rm -Rf ./dist && webpack --watch --progress --config ./webpack-dev.config.js",
    "build:production": "npm run prettier:check && rm -Rf ./dist && webpack --config ./webpack-prod.config.js",
    "prettier:check": "prettier -c ./webpack*.js ./src/js/**/* ./src/scss/**/*",
    "prettier:write": "prettier --write ./webpack*.js ./src/js/**/* ./src/scss/**/*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.2",
    "webpack-assets-manifest": "^5.0.1",
    "webpack-cli": "^4.5.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2"
  }
}